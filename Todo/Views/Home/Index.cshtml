@model TodoViewModel

        <div class="row justify-content-center"></div>
     <div class="col-xl-6 col-lg-8 col-md-8 col-sm-10">
         <table class="table">
            <thread>
                <tr>
                     <th scope="col">Id</th>
                    <th scope="col">Name</th>
                    </tr>
            </thread>
            <tbody>
                @foreach(var t in Model.TodoList)
                {
                    <tr>
                        <td class="todos-column" scope="row">@t.Id</td>
                        <td class="todos-column" scope="row">@t.Name</td>
                        <td class="buttons-column">
                            <input type="submit" value="Delete" onClick="deleteTodo(@t.Id)">
                            <input type="submit" value="Update" onClick="populateForm(@t.Id)">
                        </td>
                    </tr>
                }
            </tbody>
        </table>
         </div>
</div>